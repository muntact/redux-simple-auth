{
  "name": "rsa-oauth2-password-grant",
  "version": "0.5.0",
  "description": "An authenticator for the oauth2 password grant flow",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src test",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir lib",
    "build:commonjs:watch": "yarn run build:commonjs -- --watch",
    "build": "yarn run build:commonjs",
    "build:watch": "yarn run build:commonjs:watch",
    "precommit": "lint-staged",
    "prepublish": "yarn run clean && yarn run lint && yarn run test && yarn run build"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerelmiller/redux-simple-auth.git"
  },
  "keywords": [
    "redux",
    "authentication",
    "oauth2",
    "password grant",
    "native",
    "web"
  ],
  "author": "Orchestral Development",
  "license": "MIT",
  "dependencies": {
    "js-base64": "^2.4.0",
    "lodash.isarray": "^4.0.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isundefined": "^3.0.0",
    "redux-simple-auth": "0.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.0",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^1.0.2",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^22.1.4",
    "fetch-mock": "^5.9.4",
    "lint-staged": "^6.1.1",
    "lodash.omit": "^4.5.0",
    "prettier": "1.10.2",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.1.0",
    "rimraf": "^2.6.1",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "setupFiles": [
      "./test/setupTests.js"
    ]
  }
}
